<odoo>

  <record id="action_wizard_contract_transfer_project_issue_picking" model="ir.actions.act_window">
    <field name="name">[commown] Transfer device to another contract</field>
    <field name="src_model">project.issue</field>
    <field name="res_model">project.issue.contract_transfer.wizard</field>
    <field name="view_type">form</field>
    <field name="view_mode">form</field>
    <field name="target">new</field>
    <field name="context">{'default_issue_id': active_id}</field>
  </record>

  <record id="action_menu_wizard_contract_transfer_project_issue_picking" model="ir.values">
    <field name="name">[commown] Transfer device to another contract</field>
    <field name="model">project.issue</field>
    <field name="key">action</field>
    <field name="key2">client_action_multi</field>
    <field
        name="value"
        eval="'ir.actions.act_window,' + str(ref('action_wizard_contract_transfer_project_issue_picking'))"/>
  </record>

  <record id="action_wizard_outward_project_issue_picking" model="ir.actions.act_window">
    <field name="name">[commown] Send a device</field>
    <field name="src_model">project.issue</field>
    <field name="res_model">project.issue.outward.picking.wizard</field>
    <field name="view_type">form</field>
    <field name="view_mode">form</field>
    <field name="target">new</field>
    <field name="context">{'default_issue_id': active_id}</field>
  </record>

  <record id="action_menu_wizard_outward_project_issue_picking" model="ir.values">
    <field name="name">[commown] Open Send a device action</field>
    <field name="model">project.issue</field>
    <field name="key">action</field>
    <field name="key2">client_action_multi</field>
    <field
        name="value"
        eval="'ir.actions.act_window,' + str(ref('action_wizard_outward_project_issue_picking'))"/>
  </record>

  <record id="action_wizard_inward_project_issue_picking" model="ir.actions.act_window">
    <field name="name">[commown] Receive a device</field>
    <field name="src_model">project.issue</field>
    <field name="res_model">project.issue.inward.picking.wizard</field>
    <field name="view_type">form</field>
    <field name="view_mode">form</field>
    <field name="target">new</field>
    <field name="context">{'default_issue_id': active_id}</field>
  </record>

  <record id="action_menu_wizard_inward_project_issue_picking" model="ir.values">
    <field name="name">[commown] Open Receive a device action</field>
    <field name="key2">client_action_multi</field>
    <field
        name="value"
        eval="'ir.actions.act_window,' + str(ref('action_wizard_inward_project_issue_picking'))">
    </field>
    <field name="key">action</field>
    <field name="model">project.issue</field>
  </record>

  <record id="action_scrap_issue_device" model="ir.actions.server">
    <field name="name">[commown] Scrap involved device</field>
    <field name="model_id" ref="project_issue.model_project_issue"/>
    <field name="sequence">5</field>
    <field name="state">code</field>
    <field name="code">action = record.action_scrap_device()</field>
  </record>

  <record id="action_menu_scrap_issue_device" model="ir.values">
    <field name="name">[commown] Scrap involved device</field>
    <field name="key2">client_action_multi</field>
    <field
        name="value"
        eval="'ir.actions.server,' + str(ref('action_scrap_issue_device'))">
    </field>
    <field name="key">action</field>
    <field name="model">project.issue</field>
  </record>

</odoo>
