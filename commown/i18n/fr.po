#. module: commown
#: model:mail.template,body_html:commown.email_send_invoice_from_contract
msgid "\n"
"<p>Bonjour ${object.partner_id.firstname},</p>\n"
"<br/>\n"
"<p>Votre facture <strong>${object.number}</strong>\n"
"% if object.origin:\n"
"(référence : ${object.origin} )\n"
"% endif\n"
"d'un montant de <strong>${object.amount_total} ${object.currency_id.symbol}</strong> est disponible dans votre espace Commown.</p>\n"
"\n"
"<br><br>\n"
"<div>\n"
"  <center>\n"
"    <a href=\"https://shop.commown.fr/my/invoices\" style=\"background-color: #1abc9c; padding: 20px; text-decoration: none; color: #fff; border-radius: 5px; font-size: 16px;\" title=\"\">Accéder à mes factures</a>\n"
"  </center>\n"
"</div>\n"
"\n"
"<br><br>\n"
"\n"
"% if object.state=='paid':\n"
"    <p>Cette facture est déjà payée.</p>\n"
"% else:\n"
"    <p>Veuillez effectuer le paiement dès que possible.</p>\n"
"% endif\n"
"\n"
"<p>Merci,</p>\n"
"\n"
"<p>--</p>\n"
"\n"
"<div>\n"
"  <div>L'équipe Commown<br></div>\n"
"</div>\n"
""
msgstr "\n"
"<p style=\"margin:0px 0px 9px 0px;font-size:13px;font-family:&quot;Lucida Grande&quot;, Helvetica, Verdana, Arial, sans-serif;\">Bonjour ${object.partner_id.firstname},</p>\n"
"<br>\n"
"<p style=\"margin:0px 0px 9px 0px;font-size:13px;font-family:&quot;Lucida Grande&quot;, Helvetica, Verdana, Arial, sans-serif;\">Votre facture <strong style=\"font-weight:bold;\">${object.number}</strong>\n"
"% if object.origin:\n"
"(contrat : ${object.origin} )\n"
"% endif\n"
"d'un montant de <strong style=\"font-weight:bold;\">${object.amount_total} ${object.currency_id.symbol}</strong> est disponible dans votre <a href=\"https://shop.commown.coop/my/invoices\" target=\"_blank\" style=\"text-decoration-thickness:auto;color:rgb(124, 123, 173);background-color:transparent;\">Espace Personnel</a> en ligne Commown.</p>\n"
"\n"
"<br><br>\n"
"<div>\n"
"  <center>\n"
"    <a href=\"https://shop.commown.fr/my/invoices\" style=\"text-decoration-thickness:auto;color:rgb(124, 123, 173);background-color: #1abc9c; padding: 20px; text-decoration: none; color: #fff; border-radius: 5px; font-size: 16px;\" title=\"\">Accéder à mes factures</a>\n"
"  </center>\n"
"</div>\n"
"\n"
"<br><br>\n"
"\n"
"&nbsp;<p style=\"margin:0px 0px 9px 0px;font-size:13px;font-family:&quot;Lucida Grande&quot;, Helvetica, Verdana, Arial, sans-serif;\">Cette facture est payée par prélèvement automatique.</p><p style=\"margin:0px 0px 9px 0px;font-size:13px;font-family:&quot;Lucida Grande&quot;, Helvetica, Verdana, Arial, sans-serif;\"><br></p><p style=\"margin:0px 0px 9px 0px;font-size:13px;font-family:&quot;Lucida Grande&quot;, Helvetica, Verdana, Arial, sans-serif;\">Coopérativement,</p>\n"
"\n"
"<p style=\"margin:0px 0px 9px 0px;font-size:13px;font-family:&quot;Lucida Grande&quot;, Helvetica, Verdana, Arial, sans-serif;\">--</p>\n"
"\n"
"<div>\n"
"  <div>L'équipe Commown<br></div>\n"
"</div>\n"
""

