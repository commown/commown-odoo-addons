<odoo>

  <data noupdate="1">

    <record id="action_print_outward_label_lead" model="ir.actions.server">
      <field name="name">[commown] Print related product label(s)</field>
      <field name="model_id" ref="model_crm_lead" />
      <field name="sequence">5</field>
      <field name="state">code</field>
      <field name="code">
labels = (records or [record]).parcel_labels()

action = {
    "type": "ir.actions.act_url",
    "url": labels.website_url,
    "target": "new",
    }
      </field>
    </record>

    <record id="action_print_outward_fp2_label_task" model="ir.actions.server">
      <field
        name="name"
      >[commown] Print Fairphone parcel outward label(s)</field>
      <field name="model_id" ref="model_project_task" />
      <field name="sequence">5</field>
      <field name="state">code</field>
      <field name="code">
label = (records or [record]).parcel_labels('fp2-outward-ins300')

action = {
    "type": "ir.actions.act_url",
    "url": label.website_url,
    "target": "new",
    }
      </field>
    </record>

    <record id="action_print_return_fp2_label_task" model="ir.actions.server">
      <field name="name">[commown] Print Fairphone parcel return label</field>
      <field name="model_id" ref="model_project_task" />
      <field name="sequence">5</field>
      <field name="state">code</field>
      <field name="code">
label = record.parcel_labels('fp2-return-ins300', force_single=True)

action = {
    "type": "ir.actions.act_url",
    "url": label.website_url,
    "target": "new",
    }
      </field>
    </record>

  </data>

</odoo>
